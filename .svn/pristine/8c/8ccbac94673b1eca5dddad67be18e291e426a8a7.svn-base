package kr.happyjob.study.employee.model;

public class EmpPayHistModel {
	
	private String loginID;		// 사번
	private String name;		// 사원명
	private String grade_name;
	private String workyear;
	private String an_salary;
	private String salary;
	private String ip;
	private String hi;
	private String ii;
	private String ei;
	private String it;
	private String act_salary;
	private String pay_yn;
	
	public String getLoginID() {
		return loginID;
	}
	public void setLoginID(String loginID) {
		this.loginID = loginID;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getGrade_name() {
		return grade_name;
	}
	public void setGrade_name(String grade_name) {
		this.grade_name = grade_name;
	}
	public String getWorkyear() {
		return workyear;
	}
	public void setWorkyear(String workyear) {
		this.workyear = workyear;
	}
	public String getAn_salary() {
		return an_salary;
	}
	public void setAn_salary(String an_salary) {
		this.an_salary = an_salary;
	}
	public String getSalary() {
		return salary;
	}
	public void setSalary(String salary) {
		this.salary = salary;
	}
	public String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public String getHi() {
		return hi;
	}
	public void setHi(String hi) {
		this.hi = hi;
	}
	public String getIi() {
		return ii;
	}
	public void setIi(String ii) {
		this.ii = ii;
	}
	public String getEi() {
		return ei;
	}
	public void setEi(String ei) {
		this.ei = ei;
	}
	public String getIt() {
		return it;
	}
	public void setIt(String it) {
		this.it = it;
	}
	public String getAct_salary() {
		return act_salary;
	}
	public void setAct_salary(String act_salary) {
		this.act_salary = act_salary;
	}
	public String getPay_yn() {
		return pay_yn;
	}
	public void setPay_yn(String pay_yn) {
		this.pay_yn = pay_yn;
	}
}
